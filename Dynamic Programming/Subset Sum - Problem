#include <bits/stdc++.h>
using namespace std;

int main()
{
    int n;
    cin>>n;
    int* w = new int[n];
    for(int i=0;i<n;i++) cin>>w[i];
    int k;
    cin>>k;
    bool** dp = new bool*[2];
    for(int i=0;i<2;i++)
        dp[i] = new bool[k+1];
    for(int i=0;i<=k;i++) dp[0][i] = false;
    dp[0][0] = true;
    int flag = 1;
    for(int i=1;i<=n;i++){
        for(int j=1;j<=k;j++){
            dp[flag][j] = dp[flag^1][j];
            if(w[i-1]<=j) dp[flag][j] = dp[flag][j] | dp[flag^1][j-w[i-1]];
        }
        flag = flag^1;
    }
    int ans = dp[flag^1][k];
    for(int i=0;i<2;i++)
        delete[] dp[i];
    delete[] dp;
    if(ans==0) cout<<"No"<<endl;
    else cout<<"Yes"<<endl;
    return 0;
}
